{
  "expo": {
    "name": "TextAssist",
    "slug": "TextAssist",
    "version": "1.0.0",
    "orientation": "portrait",
    "permissions": [
      "CAMERA",
      "CAMERA_ROLL"
    ],
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pulcovamon.TextAssist"
    },
    "android": {
      "package": "com.pulcovamon.TextAssist"
    },
    "web": {},
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "eeaba185-bd69-4dc0-bd2f-99c8486e01b6"
      }
    }
  }
}
